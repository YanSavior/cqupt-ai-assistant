{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/Picture1.png",
      "dest": "/Picture1.png"
    },
    {
      "src": "/Picture2.png", 
      "dest": "/Picture2.png"
    },
    {
      "src": "/Picturee.png",
      "dest": "/Picturee.png"
    },
    {
      "src": "/Picture.jpg",
      "dest": "/Picture.jpg"
    },
    {
      "src": "/favicon.ico",
      "dest": "/favicon.ico"
    },
    {
      "src": "/manifest.json",
      "dest": "/manifest.json"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "REACT_APP_API_URL": "https://api.deepseek.com"
  }
} 